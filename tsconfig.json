{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@*": ["./*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/dashboard/page.tsx", "components/main/page.js", "util/auth.js", "components/layout/header/page.tsx", "app/api/auth/[...nextauth]/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
